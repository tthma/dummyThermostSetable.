{
    "name": "homebridge-dummythermostsetable",
    "version": "1.1.0",
    "description": "Plugin for homebridge to create a fake thermostat accessory",
    "main": "dist/index.js",
    "keywords": [
      "homebridge-plugin"
    ],
    "author": "Mat",
    "license": "Unlicense",
    "bugs": {
      "url": "https://github.com/tthma/dummyThermostSetable/issues"
    },
    "homepage": "https://github.com/tthma/dummyThermostSetable#readme",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/tthma/dummyThermostSetable..git"
    },
    "scripts": {
      "format": "prettier --write src/",
      "watch": "npm run build && npm link && nodemon",
